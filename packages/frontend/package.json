{
  "name": "frontend",
  "private": true,
  "scripts": {
    "dev": "pnpm gen && FORCE_COLOR=true run-p -l -c dev:*",
    "dev:serve": "next dev",
    "dev:gql-codes": "pnpm gen:gql-codes -- --watch",
    "dev:gql-schema": "pnpm gen:gql-schema",
    "dev:chakra-ui-typings": "pnpm gen:chakra-ui-typings",
    "dev:path": "pnpm gen:path -- --watch",
    "gen": "FORCE_COLOR=true run-s gen:*",
    "gen:gql-codes": "./scripts/gen_gql_codes.sh",
    "gen:gql-schema": "node ./scripts/gen_gql_schema.mjs",
    "gen:chakra-ui-typings": "chakra-cli tokens src/libs/theme.ts",
    "gen:path": "pathpida -o src/libs --ignorePath .gitignore",
    "build": "next build && ./scripts/post_build.sh",
    "start": "next start",
    "lint": "FORCE_COLOR=true run-p -l -c lint:*",
    "lint:next": "next lint --max-warnings=0 --format codeframe",
    "lint:src-typing": "tsc --pretty",
    "lint:cypress-typing": "tsc -p cypress --pretty",
    "lint:format": "prettier -c './**/*.{js,ts,tsx}'",
    "format": "prettier -w './**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@urql/exchange-graphcache": "^4.3.6",
    "framer-motion": "^4",
    "graphql-anywhere": "^4.2.7",
    "next": "^12.1.0",
    "next-urql": "^3.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^8.33.1",
    "subscriptions-transport-ws": "^0.11.0",
    "urql": "^2.1.3"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.8.2",
    "@chakra-ui/styled-system": "^1.18.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typed-document-node": "^2.2.5",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-urql": "^3.5.3",
    "@graphql-codegen/typescript-urql-graphcache": "^2.2.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@homebound/graphql-typescript-factories": "^2.48.0",
    "@percy/cli": "^1.0.0-beta.75",
    "@percy/core": "^1.0.0-beta.75",
    "@percy/cypress": "^3.1.1",
    "@swc/core": "^1.2.144",
    "@swc/jest": "^0.2.17",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.25",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@urql/introspection": "^0.3.2",
    "cypress": "^9.5.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "http-proxy-middleware": "^2.0.3",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^27.5.1",
    "jest-css-modules": "^2.1.0",
    "msw": "^0.36.8",
    "node-fetch": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
